# openstack charms that have ns in cs:
for c in ${OST_CHARMS[@]} ${OVN_CHARMS[@]} ${CEPH_CHARMS[@]}; do
    CS_NS_CHARMS[$c]=~openstack-charmers-next/
done
CS_NS_CHARMS[rabbitmq-server]=~openstack-charmers-next/
CS_NS_CHARMS[percona-cluster]=~openstack-charmers-next/
CS_NS_CHARMS[mysql-router]=~openstack-charmers-next/
CS_NS_CHARMS[mysql-innodb-cluster]=~openstack-charmers-next/
CS_NS_CHARMS[vault]=~openstack-charmers-next/
CS_NS_CHARMS[hacluster]=~openstack-charmers-next/

